    "from math import log, sqrt\n",
    "\n",
    "def bad_formula(a, b):\n",
    "    return (((log(a) + log(b))/5)**2)/(2+sqrt(3))\n",
    "\n",
    "def good_formula(a, b):\n",
    "    t = 0.2*log(a*b)\n",
    "    return 0.2679491924311227*t*t # 1/(2+sqrt(3))\n",
    "\n",
    "%timeit bad_formula(3, 4)\n",
    "%timeit good_formula(3.0, 4.0)"
   ]
